<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="WebApiUrl" value="http://localhost:29409/simple/data/sync"/>
    <add key="LogErrorTextFileName" value="SimpleSyncErrorLog"/>
    <add key="LogInfoTextFileName" value="SimpleSyncInfoLog"/>
    <add key="TimerInterval" value="15000"/>
  </appSettings>
  <connectionStrings>
    <add name="SqlServerConnectionString" connectionString="Data Source=.\sqlexpress;Initial Catalog=simplesyncclientdb;Persist Security Info=True;User ID=sa;Password=0123456789" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>